{
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},
	"description": "Build an Extension!",
	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	},
	"manifest_version": 2,
	"name": "Getting Started Example",
	"page_action": {
		"default_icon": {
			"16": "images/get_started16.png",
			"32": "images/get_started32.png",
			"48": "images/get_started48.png",
			"128": "images/get_started128.png"
		},
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"declarativeContent",
		"storage"
	],
	"version": "1.0"
}
